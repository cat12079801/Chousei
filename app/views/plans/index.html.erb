<p id="notice"><%= notice %></p>

<h1>Listing Plans</h1>

<table>
  <thead>
    <tr>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @plans.each do |plan| %>
      <tr>
        <td><%= plan.title %></td>
        <td><%= link_to 'Show', plan_path(plan.original_url) %></td>
        <td><%= link_to 'Edit', edit_plan_path(plan.original_url) %></td>
        <td><%= link_to 'Destroy', plan_path(plan.original_url), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to new_plan_path do%>
  <paper-button raised>New Plan</paper-button>
<% end %>
